{
  "name": "taco-mdt-aa-signing-demo",
  "version": "0.1.0",
  "description": "A demo showing TACo distributed signing with Account Abstraction wallets",
  "private": true,
  "author": "NuCypher <dev@nucypher.com>",
  "scripts": {
    "check": "tsx --no-warnings src/server.ts --dry-run 2>/dev/null && echo 'âœ“ Syntax check passed'",
    "start": "node dist/server.js",
    "build": "esbuild src/server.ts --bundle --platform=node --outfile=dist/server.js --packages=external",
    "dev": "tsx watch src/server.ts",
    "demo:legacy": "tsx scripts/index.ts",
    "type-check": "echo \"Note: Full type-check skipped due to viem/delegation-toolkit type incompatibilities\"",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@metamask/delegation-toolkit": "^0.11.0",
    "@metamask/delegation-utils": "^0.11.0",
    "@nucypher/shared": "0.6.0-alpha.5",
    "@nucypher/taco": "0.7.0-alpha.5",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ethers": "^5.8.0",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "permissionless": "^0.2.54",
    "viem": "2.34.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.17.9",
    "esbuild": "^0.27.2",
    "prettier": "^3.7.4",
    "tsx": "^4.20.4",
    "typescript": "^5.7.2"
  }
}